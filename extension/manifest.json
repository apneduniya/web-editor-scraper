{
    "manifest_version": 3,
    "name": "Web Editor Scraper",
    "description": "An extension for scraping places from webpages.",
    "version": "1.0",
    "action": {
        "default_title": "Web Editor Scraper",
        "default_popup": "index.html"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    }
}